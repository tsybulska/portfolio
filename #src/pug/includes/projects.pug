            .projects__img.projects-img
                .projects-img__slider
                    .projects-img__icon
                    .projects-img__slide(data-slide='1').slide-active
                        picture
                            source(class='lazyload' data-srcset='./assets/img/js-filter-calculator.webp, ./assets/img/js-filter-calculator-640.webp 640w' type='image/webp')
