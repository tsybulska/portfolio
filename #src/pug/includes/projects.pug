        span Projects
    input#All(type='radio' name='categories' value='All' checked='')
    input#HTML(type='radio' name='categories' value='HTML')
    input#JavaScript(type='radio' name='categories' value='JavaScript')
    input#ReactJS(type='radio' name='categories' value='ReactJS')
